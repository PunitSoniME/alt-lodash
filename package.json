{
  "name": "alt-lodash",
  "version": "0.1.0-rc.2",
  "description": "This library can be used as alternate of lodash",
  "author": "Punit Soni <punit.soni33@gmail.com> (https://punit-soni.web.app)",
  "license": "MIT",
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "test:watch": "tsdx test --watch",
    "test:coverage": "tsdx test --coverage",
    "test:coverage:run": "yarn run test:coverage && serve ./coverage/lcov-report",
    "lint": "tsdx lint",
    "lint-fix": "tsdx lint --fix",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "prerelease": "npm version prerelease && npm publish && git push"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/PunitSoniME/alt-lodash.git"
  },
  "bugs": {
    "url": "https://github.com/PunitSoniME/alt-lodash/issues"
  },
  "main": "dist/index.js",
  "module": "dist/alt-lodash.esm.js",
  "engines": {
    "node": ">=10"
  },
  "keywords": [
    "lodash alternative",
    "alt lodash",
    "array",
    "helpers",
    "methods",
    "javascript methods"
  ],
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": true
  },
  "size-limit": [
    {
      "path": "dist/alt-lodash.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/alt-lodash.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@rollup/plugin-replace": "^4.0.0",
    "@size-limit/preset-small-lib": "^8.1.0",
    "@types/lodash": "^4.14.190",
    "husky": "^8.0.2",
    "lodash": "^4.17.21",
    "size-limit": "^8.1.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.4.1",
    "typescript": "^4.9.3"
  },
  "resolutions": {
    "@typescript-eslint/eslint-plugin": "^5.45.1",
    "@typescript-eslint/parser": "^5.45.1"
  }
}
